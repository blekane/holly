pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                sleep 5
            }
        } 
        stage('build') {
            steps {
                echo 'Hello Build'
                sleep 5
            }
        }
        
         stage('deploy') {
            steps {
                echo 'Hello Deploy'
                sleep 5
            }
        }
        
        
       stage('test') {
            steps {
                echo 'Hello Test'
                sleep 5
            }
        }  
        
    }
}

